<p id="notice"><%= notice %></p>

<p>
  <strong>Codigo articulo:</strong>
  <%= @articulo.codigo_articulo %>
</p>

<p>
  <strong>Descripcion:</strong>
  <%= @articulo.descripcion %>
</p>



<p>
  <strong>Unidad compra diaria:</strong>
  <%= @articulo.unidad_compra_diaria %>
</p>


<p>
  <strong>Total en inventario:</strong>
  <%= @articulo.total_inventario %>
</p>

<p>
  <strong>Suplidores:</strong>
  <% if @articulo.suplidores.respond_to? :each %>
      <% @articulo.suplidores.each do |x| %>
        <pre><%= JSON.pretty_generate(x) %></pre>
    <% end %>
<% end %>

<p>

  <%= link_to 'Edit', edit_articulo_path(@articulo) %> |
<%= link_to 'Back', articulos_path %>
